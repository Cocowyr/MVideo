GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/kxmovie"
LIBRARY_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/kxmovie/output"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/kxmovie"
OTHER_LDFLAGS = $(inherited) -ObjC -l"avcodec" -l"avformat" -l"avutil" -l"iconv" -l"kxmovie" -l"swresample" -l"swscale" -l"z" -framework "Accelerate" -framework "AudioToolbox" -framework "CoreAudio" -framework "MediaPlayer" -framework "OpenGLES" -framework "QuartzCore"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
